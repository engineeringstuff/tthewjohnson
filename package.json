{
	"name": "tthewjohnson",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"deploy": "wrangler pages deploy ./.svelte-kit/cloudflare --project-name tthewjohnson --commit-dirty=true",
		"lint": "eslint",
		"check": "svelte-check",
		"test": "vitest",
		"format": "prettier --write \"./*.{css,svelte,ts,js,json,md}\" \"./**/*.{css,svelte,ts,js,json,md}\"",
		"clean:svelte": "find . -name '.svelte-kit' -type d -prune -print -exec rm -rf '{}' \\;",
		"clean:modules": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;"
	},
	"type": "module",
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.5.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/node": "^20.11.20",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"autoprefixer": "^10.4.17",
		"esbuild": "0.20.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.4",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "5.3.3",
		"vite": "^5.1.4",
		"vitest": "^1.3.1",
		"wrangler": "^3.29.0"
	}
}
