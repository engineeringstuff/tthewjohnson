{
	"name": "tthewjohnson",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"deploy": "wrangler pages publish ./.svelte-kit/cloudflare --project-name tthewjohnson --commit-dirty=true",
		"lint": "eslint",
		"check": "svelte-check",
		"test": "vitest",
		"format": "prettier --write \"./*.{css,svelte,ts,js,json,md}\" \"./**/*.{css,svelte,ts,js,json,md}\"",
		"clean:svelte": "find . -name '.svelte-kit' -type d -prune -print -exec rm -rf '{}' \\;",
		"clean:modules": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
		"clean:yarn": "find . -name 'yarn.lock' -delete; find . -name 'yarn-error.log' -delete",
		"clean": "yarn --silent clean:svelte; yarn --silent clean:yarn; yarn --silent clean:modules"
	},
	"type": "module",
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^2.2.2",
		"@sveltejs/kit": "^1.16.3",
		"@sveltejs/vite-plugin-svelte": "^2.2.0",
		"@types/node": "^20.1.5",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"autoprefixer": "^10.4.14",
		"esbuild": "0.17.19",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-inline-svg": "^1.2.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "4.9.5",
		"vite": "^4.3.6",
		"vitest": "^0.31.0",
		"wrangler": "^2.20.0"
	},
	"packageManager": "yarn@3.5.1"
}
